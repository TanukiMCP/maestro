{
  "name": "maestro-mcp-server",
  "version": "2.0.0",
  "description": "Maestro Intelligence Amplification Engine - Advanced MCP Server with Computational Orchestration",
  "main": "mcp_stdio_server.py",
  "scripts": {
    "start": "python mcp_stdio_server.py",
    "start:enhanced": "python maestro_enhanced.py",
    "test": "python scripts/test_client_session.py",
    "test:enhanced": "python scripts/test_enhanced_server.py",
    "test:all": "python scripts/test_client_session.py && python scripts/test_enhanced_server.py"
  },
  "keywords": [
    "mcp",
    "mcp-server",
    "intelligence-amplification",
    "computational-orchestration",
    "ai",
    "reasoning",
    "decision-support",
    "collaborative-intelligence"
  ],
  "author": "Maestro Intelligence Systems",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/maestro-mcp-server.git"
  },
  "engines": {
    "python": ">=3.11"
  },
  "mcp": {
    "server": {
      "command": "python",
      "args": ["maestro_enhanced.py"],
      "transport": "stdio"
    }
  },
  "dependencies": {
    "mcp": "^1.9.2"
  },
  "devDependencies": {},
  "files": [
    "mcp_stdio_server.py",
    "maestro_enhanced.py",
    "requirements.txt",
    "README.md"
  ]
} 